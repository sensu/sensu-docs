{
    "root": "public/",
    "https_only": true,
    "error_page": "/404.html",
    "rewrites": {
        "~ ^/sensu-core/latest/?(.*)$": {
            "url": "/sensu-core/1.4/$1",
            "status": 302
        },
        "~ ^/sensu-enterprise/latest/?(.*)$": {
            "url": "/sensu-enterprise/3.0/$1",
            "status": 302
        },
        "~ ^/sensu-enterprise-dashboard/latest/?(.*)$": {
            "url": "/sensu-enterprise-dashboard/2.11/$1",
            "status": 302
        },
        "~ ^/plugins/latest/?(.*)$": {
            "url": "/plugins/1.0/$1",
            "status": 302
        }
    }
}
