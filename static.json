{
    "root": "/app/public/",
    "https_only": true,
    "error_page": "/404.html",
    "pattern_redirects": {
        "~ ^/sensu-core/latest/?(.*)$": {
            "url": "/sensu-core/1.8/$1",
            "status": 301
        },
        "~ ^/uchiwa/latest/?(.*)$": {
            "url": "/uchiwa/1.0/$1",
            "status": 301
        },
        "~ ^/sensu-enterprise/latest/?(.*)$": {
            "url": "/sensu-enterprise/3.6/$1",
            "status": 301
        },
        "~ ^/sensu-enterprise-dashboard/latest/?(.*)$": {
            "url": "/sensu-enterprise-dashboard/2.16/$1",
            "status": 301
