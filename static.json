{
    "root": "/app/public/",
    "https_only": true,
    "error_page": "/404.html",
    "pattern_redirects": {
        "~ ^/sensu-core/2.0/?(.*)$": {
            "url": "/sensu-go/latest/$1",
            "status": 301
        },
        "~ ^/sensu-core/latest/?(.*)$": {
            "url": "/sensu-core/1.8/$1",
            "status": 301
        },
        "~ ^/uchiwa/latest/?(.*)$": {
            "url": "/uchiwa/1.0/$1",
            "status": 301
        },
        "~ ^/sensu-enterprise/latest/?(.*)$": {
            "url": "/sensu-enterprise/3.6/$1",
            "status": 301
        },
        "~ ^/sensu-enterprise-dashboard/latest/?(.*)$": {
            "url": "/sensu-enterprise-dashboard/2.16/$1",
            "status": 301
        },
        "~ ^/sensu-go/latest/guides/create-a-ready-only-user$": {
            "url": "/sensu-go/latest/guides/create-read-only-user",
            "status": 301
        },
        "~ ^/sensu-go/5.15/?(?<=\/)(.*)$": {
            "url": "/sensu-go/latest/$1",
            "status": 302
        },
        "~ ^/sensu-go/5.1[0-1]/?(?<=\/)(.*)$": {
            "url": "/sensu-go/latest/$1",
            "status": 301
        },
        "~ ^/sensu-go/5.[0-9]/?(?<=\/)(.*)$": {
            "url": "/sensu-go/latest/$1",
            "status": 301
        },
        "~ ^/plugins/latest/?(.*)$": {
            "url": "/plugins/1.0/$1",
            "status": 301
        }
    }
}
