{
    "root": "/app/public/",
    "https_only": true,
    "error_page": "/404.html",
    "rewrites": {
        "~ ^/sensu-core/latest/?(.*)$": {
            "url": "/sensu-core/1.6/$1",
            "status": 302
        },
        "~ ^/uchiwa/latest/?(.*)$": {
            "url": "/uchiwa/1.0/$1",
            "status": 302
        },
        "~ ^/sensu-enterprise/latest/?(.*)$": {
            "url": "/sensu-enterprise/3.2/$1",
            "status": 302
        },
        "~ ^/sensu-enterprise-dashboard/latest/?(.*)$": {
            "url": "/sensu-enterprise-dashboard/2.12/$1",
            "status": 302
        },
        "~ ^/plugins/latest/?(.*)$": {
            "url": "/plugins/1.0/$1",
            "status": 302
        }
    }
}
